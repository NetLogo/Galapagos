<?xml version="1.0" encoding="utf-8"?>
<model version="NetLogo 7.0.0" snapToGrid="true">
  <code>breed [ grains grain ]
breed [ nodes node ]

to setup
  clear-all

  ; There is a node in the center by default because it makes the
  ; patterns more even and symmetrical. Feel free to change this
  ; line and see how it affects the resulting patterns!
  create-nodes 1 [ init-node ]

  ; Create the the rest of the nodes and distribute them
  ; evenly in a circle around the center of the world
  ; (create-ordered-nodes automatically evenly spaces their headings)
  create-ordered-nodes (num-nodes - 1) [
    init-node
    fd max-pxcor
  ]

  ; create the specified number of grains and distribute them
  ; randomly across the world
  create-grains num-grains [
    setxy random-xcor random-ycor
    set shape "circle"
    set size 0.25
    set color [ 255 128 128 ]
  ]

  reset-ticks
end

; node procedure to initialize visuals
to init-node
  set shape "circle"
  set size 1.5
  set color [ 200 128 255 150 ]
end

; in go, each grain calculates its speed based on the surrounding
; nodes, and moves at that speed in a random direction
to go
  ask grains [
    ; move in a random direction which mimics behavior of grains on a vibrating plate
    set heading random-float 360
    let speed sum [ calc-speed-contribution ] of nodes ; calculate speed based on contribution from each node
    forward speed
  ]

  tick
end

; This reporter calculates the speed contribution to a grain of sand from
; a single node. The primary factor is the sine of the distance between
; the node and the grain, which replicates the periodicity of the resonant
; waves that appear in the vibrating plate in the original experiment.
to-report calc-speed-contribution
  report amplitude * sin (frequency * distance myself) ; "myself" is a grain in this context
end


